//10952
#include <stdio.h>

int main()
{
	int a = 1;
	int b = 2;
	while (1) {
		scanf_s("%d %d", &a, &b);
		if (a == 0 && b == 0) break;
		printf("%d\n", a + b);
	}
	return 0;
} 

//8393
#include <stdio.h>

int main() {
	int n;
	scanf_s("%d", &n);
	int sum = 0;
	for (int i = 1; i <= n; ++i) {
		sum += i;
	}
	printf("%d\n", sum;);

	retutn 0;
} 
//11022
#include <stdio.h>

int main(void) {
	int t;

	scanf("%d", &t);

	int a, b;

	for (int i = 1; i <= t; i++) {
		scanf("%d %d", &a, &b);
		printf("Case #%d: %d + %d = %d\n", i, a, b, a + b);
	}

	return 0;
} 
