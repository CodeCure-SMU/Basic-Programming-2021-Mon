// 과제1 
#include <stdio.h>
int main(void)
{
	int n;
	int a=1;
	scanf("%d", &n);
	
	while(a<10){
		printf("%d * %d = %d\n", n, a, n*a);
		a++;
	}
	return 0;
}


// 과제2 
#include <stdio.h>
int main(void)
{
	int input, i, j;
	scanf("%d", &input);
	
	for(i=1; i<=input; i++){
		for(j=1; j<=i; j++){
			printf("*");
		}
	printf("\n");
	}
	return 0;
}

 
// 과제3
#include <stdio.h>
int main(void)
{
	int a=1, b=1;
	
	while(1){
	scanf("%d %d", &a, &b);
	
	if(a==0 && b==0)
		break;
	printf("%d\n", a+b);
	}
		
	return 0;
} 


// 과제4
#include <stdio.h>
int main(void)
{
	int input, i, sum=0;
	scanf("%d", &input);
	
	for(i=1; i<=input; i++){
		sum += i;
	}
	printf("%d", sum);
	return 0;
} 
