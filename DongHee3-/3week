// Baekjoon2739
#include <stdio.h>
 
int main() {
    int n;
    scanf("%d", &n);
    for (int i = 1; i < 10; ++i) {
        printf("%d * %d = %d\n", n, i, n * i);
    }
    return 0;
}

// Baekjoon_10952
#include <stdio.h>

int main() {
	int a=1, b=1;
	while (1) {
		scanf("%d %d", &a, &b);
		if (a == 0 && b == 0) break;
		printf("%d\n", a + b);
	}return 0;
}

// Baekjoon8393
#include <stdio.h>
 
int main() {
    int n;
    scanf("%d", &n);
 
    int sum = 0;
    for (int i = 1; i <= n; ++i) {
        sum += i;
    }
    printf("%d\n", sum);
 
    return 0;
}
